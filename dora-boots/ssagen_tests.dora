@test fun testSsaGenAddIntArguments(_x: Testing) {
    let fct = bytecode("bcSsaGenAddIntArguments");
    let graph = createGraph(fct);

    assertGraph(graph, "Block 0:
  0: Arg.Int(0) [2]
  1: Arg.Int(1) [2]
  2: Add.Int 0, 1 [3]
  3: Ret.Int 2
");
}

fun bcSsaGenAddIntArguments(a: Int, b: Int) -> Int { a + b }

