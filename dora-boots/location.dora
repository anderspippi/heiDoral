enum LocationKind {
    None,
    Constant,
    StackSlot,
    CoreRegister,
    FpuRegister,
}

class Location {
    var kind: LocationKind = LocationKind::None;
    var data: Int32 = 0;
}

class LocationData {
    var inputs: Vec[Location] = Vec[Location]();
    var temps: Vec[Location] = Vec[Location]();
    var output: Location = Location();
}
