fn main() {
    let threads = argv(0).toInt().getOrPanic();

    var i = 0;

    while i < threads {
        let thread = MyThread();
        thread.start();
        i = i + 1;
    }
}

class MyThread() extends Thread {
    @override fn run() {
        allocator();
    }
}

fn allocator() {
    var i = 0;

    while i < 20_000_000 {
        Foo();
        i = i + 1;
    }
}

