fn main() {
    let ba = Array[UInt8]::empty();
    assert(ba.size() == 0L);

    let ba = Array[UInt8]::fill(10L, 2Y);
    assert(ba.size() == 10L);

    ba(0L) = 10Y;
    ba(1L) = 11Y;
    ba(2L) = 12Y;

    assert(10Y == ba(0L));
    assert(11Y == ba(1L));
    assert(12Y == ba(2L));
    assert(2Y == ba(9L));
}