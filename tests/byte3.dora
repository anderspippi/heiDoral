//= cannon

fun main() {
    let ba = arrayEmpty[UInt8]();
    assert(ba.length() == 0L);

    let ba = arrayFill[UInt8](10L, 2Y);
    assert(ba.length() == 10L);

    ba(0L) = 10Y;
    ba(1L) = 11Y;
    ba(2L) = 12Y;

    assert(10Y == ba(0L));
    assert(11Y == ba(1L));
    assert(12Y == ba(2L));
    assert(2Y == ba(9L));
}