//= cannon

fun main() {
    let ba = arrayEmpty[UInt8]();
    assert(ba.length() == 0L);

    let ba = arrayFill[UInt8](10L, 2Y);
    assert(ba.length() == 10L);

    ba(0) = 10Y;
    ba(1) = 11Y;
    ba(2) = 12Y;

    assert(10Y == ba(0));
    assert(11Y == ba(1));
    assert(12Y == ba(2));
    assert(2Y == ba(9));
}