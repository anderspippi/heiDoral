fn main() {
    let content = std::io::readFileAsBytes("tests/io/123.bin").getOrPanic();
    assert(content.size() == 3);
    assert(content(0) == 1u8);
    assert(content(1) == 2u8);
    assert(content(2) == 3u8);

    let content = std::io::readFileAsBytes("unknown.txt");
    assert(content.isNone());
}
