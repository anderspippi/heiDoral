fn main() {
    assert(tpl1() == 1i32);
    assert(tpl2() == 2i32);
    assert(tpl3() == 3i32);
    assert(tplsum() == 6i32);
}

fn tpl1(): Int32 {
    let x = (1i32, 2i32, 3i32);
    x.0
}

fn tpl2(): Int32 {
    let x = (1i32, 2i32, 3i32);
    x.1
}

fn tpl3(): Int32 {
    let x = (1i32, 2i32, 3i32);
    x.2
}

fn tplsum(): Int32 {
    let x = (1i32, 2i32, 3i32);
    x.0 + x.1 + x.2
}
