//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo (let value: Int32)

fun main() {
    let x = Array::ofSizeUnsafe[(Int32, Foo)](1024L);
    for i in std::range(0, x.size().toInt32()) {
        x(i.toInt64()) = (i, Foo(i));
    }
    std::forceCollect();
    for i in std::range(0, x.size().toInt32()) {
        assert(x(i.toInt64()).0 == i && x(i.toInt64()).0 == x(i.toInt64()).1.value);
    }
}
