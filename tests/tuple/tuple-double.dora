fun main() {
    assert(tpl1() == 1.0);
    assert(tpl2() == 2.0);
    assert(tpl3() == 3.0);
    assert(tplsum() == 6.0);
}

fun tpl1() -> Double {
    let x = (1.0, 2.0, 3.0);
    x.0
}

fun tpl2() -> Double {
    let x = (1.0, 2.0, 3.0);
    x.1
}

fun tpl3() -> Double {
    let x = (1.0, 2.0, 3.0);
    x.2
}

fun tplsum() -> Double {
    let x = (1.0, 2.0, 3.0);
    x.0 + x.1 + x.2
}
