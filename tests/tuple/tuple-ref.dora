//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo(let value: Int32) {
    std::forceCollect();
}

fn main() {
    assert(tpl1() == 1I);
    assert(tpl2() == 2I);
    assert(tpl3() == 3I);
    assert(tplsum() == 6I);
}

fn tpl1(): Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.0.value
}

fn tpl2(): Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.1.value
}

fn tpl3(): Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.2.value
}

fn tplsum(): Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.0.value + x.1.value + x.2.value
}
