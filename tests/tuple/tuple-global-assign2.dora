//= vm-args "--gc=copy --disable-tlab --gc-stress"
//= ignore

var x: (Foo, Foo);

fun main() {
    forceCollect();
    x = (Foo(17I), Foo(42I));
    assert(x.0.value == 17I);
    assert(x.1.value == 42I);
    forceCollect();
    assert(x.0.value == 17I);
    assert(x.1.value == 42I);
}

class Foo(let value: Int32)
