//= vm-args "--gc=copy --disable-tlab --gc-stress"

var x: (Foo, Foo) = (Foo(0), Foo(0));

fun main() {
    forceCollect();
    x = (Foo(17), Foo(42));
    assert(x.0.value == 17);
    assert(x.1.value == 42);
    forceCollect();
    assert(x.0.value == 17);
    assert(x.1.value == 42);
}

class Foo(let value: Int32)
