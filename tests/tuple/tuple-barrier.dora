//= cannon
//= vm-args "--gc=swiper --gc-verify"

class TupleClass {
    var value: (Foo, Int64);
}

class Foo(let value: Int32)

fun main() {
    let tpl = TupleClass();
    forceCollect();
    tpl.value = (Foo(4711), 4712L);
    forceMinorCollect();
    assert(tpl.value.0.value == 4711);
    assert(tpl.value.1 == 4712L);
}
