//= cannon

fun main() {
  let result = Int32::minValue() / -1I;
  assert(result == Int32::minValue());

  let result = Int32::minValue() % -1I;
  assert(result == 0I);

  let result = Int64::minValue() / -1L;
  assert(result == Int64::minValue());

  let result = Int64::minValue() % -1L;
  assert(result == 0L);
}
