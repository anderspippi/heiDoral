fun main() {    
    assert(!eq(20Y, 5Y));
    assert(eq(5Y, 5Y));
    assert(!eq(5Y, 20Y));

    assert(ne(20Y, 5Y));
    assert(!ne(5Y, 5Y));
    assert(ne(5Y, 20Y));


    assert(!lt(20Y, 5Y));
    assert(!lt(5Y, 5Y));
    assert(lt(5Y, 20Y));

    assert(!le(20Y, 5Y));
    assert(le(5Y, 5Y));
    assert(le(5Y, 20Y));

    assert(ge(20Y, 5Y));
    assert(ge(5Y, 5Y));
    assert(!ge(5Y, 20Y));

    assert(gt(20Y, 5Y));
    assert(!gt(5Y, 5Y));
    assert(!gt(5Y, 20Y));
}

fun eq(x: UInt8, y: UInt8): Bool {
    return x == y;
}

fun ne(x: UInt8, y: UInt8): Bool {
    return x != y;
}

fun lt(x: UInt8, y: UInt8): Bool {
    return x < y;
}

fun le(x: UInt8, y: UInt8): Bool {
    return x <= y;
}

fun ge(x: UInt8, y: UInt8): Bool {
    return x >= y;
}

fun gt(x: UInt8, y: UInt8): Bool {
    return x > y;
}