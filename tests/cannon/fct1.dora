fn main() {
  assert(foo_bool() == true);
  assert(foo_byte() == 3Y);
  assert(foo_char() == '3');
  assert(foo_int32() == 3I);
  assert(foo_int64() == 3L);
  assert(foo_float() == 3F);
  assert(foo_double() == 3D);
  assert(foo_string() == "3");
}

fn foo_bool(): Bool {
  return bar_bool();
}

fn bar_bool(): Bool {
  return true;
}

fn foo_byte(): UInt8 {
  return bar_byte();
}

fn bar_byte(): UInt8 {
  return 3Y;
}

fn foo_char(): Char {
  return bar_char();
}

fn bar_char(): Char {
  return '3';
}

fn foo_int32(): Int32 {
  return bar_int32();
}

fn bar_int32(): Int32 {
  return 3I;
}

fn foo_int64(): Int64 {
  return bar_int64();
}

fn bar_int64(): Int64 {
  return 3L;
}

fn foo_float(): Float32 {
  return bar_float();
}

fn bar_float(): Float32 {
  return 3F;
}

fn foo_double(): Float64 {
  return bar_double();
}

fn bar_double(): Float64 {
  return 3D;
}

fn foo_string(): String {
  return bar_string();
}

fn bar_string(): String {
  return "3";
}
