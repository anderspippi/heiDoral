fun main() {
    assert(rotateLeft(-12I, 1I) == -23I);
    assert(rotateLeft(-12I, 32I) == -12I);
    assert(rotateLeft(-12I, -1I) == 2147483642I);

    assert(rotateRight(-12I, 1I) == 2147483642I);
    assert(rotateRight(-12I, 32I) == -12I);
    assert(rotateRight(-12I, -1I) == -23I);
}

@cannon fun rotateLeft(x: Int32, y: Int32) -> Int32 {
    return x.rotateLeft(y.toInt());
}

@cannon fun rotateRight(x: Int32, y: Int32) -> Int32 {
    return x.rotateRight(y.toInt());
}