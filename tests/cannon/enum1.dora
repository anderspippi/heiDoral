fun main() {    
    assert(!eq(Foo::A, Foo::B));
    assert(eq(Foo::B, Foo::B));
    assert(!eq(Foo::B, Foo::A));

    assert(ne(Foo::A, Foo::B));
    assert(!ne(Foo::B, Foo::B));
    assert(ne(Foo::B, Foo::A));
}

fun eq(x: Foo, y: Foo): Bool {
    return x == y;
}

fun ne(x: Foo, y: Foo): Bool {
    return x != y;
}

enum Foo {
    A,
    B
}