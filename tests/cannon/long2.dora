fun main() {
    assert(rotateLeft(-12L, 1I) == -23L);
    assert(rotateLeft(-12L, 64I) == -12L);
    assert(rotateLeft(-12L, -1I) == 9223372036854775802L);

    assert(rotateRight(-12L, 1I) == 9223372036854775802L);
    assert(rotateRight(-12L, 64I) == -12L);
    assert(rotateRight(-12L, -1I) == -23L);
}

@cannon fun rotateLeft(x: Int64, y: Int32) -> Int64 {
    return x.rotateLeft(y);
}

@cannon fun rotateRight(x: Int64, y: Int32) -> Int64 {
    return x.rotateRight(y);
}