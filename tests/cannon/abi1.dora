fun main() {
    assert(testInt32(3) == 3);
    assert(testInt32(122) == 122);

    assert(testFloat32(3F) == 3F);
    assert(testFloat32(122F) == 122F);

    let foo = Foo();
    assert(testPtr(foo) === foo);

    assert(foo.testSelf() === foo);
}

fun testInt32(x: Int32): Int32 {
    return x;
}

fun testFloat32(x: Float32): Float32 {
    return x;
}

fun testPtr(x: Foo): Foo {
    return x;
}

class Foo() {
    fun testSelf(): Foo {
        return self;
    }
}