fun main() {
    let a = Array[Int](10);
    assert(array_get(a, 0) == 0);

    // array_set(a, 0, 7);
    // assert(array_get(a, 0) == 7);
}

@new_call fun array_set(a: Array[Int], idx: Int, value: Int) {
    a(idx) = value;
}

@new_call fun array_get(a: Array[Int], idx: Int) -> Int {
    return a(idx);
}