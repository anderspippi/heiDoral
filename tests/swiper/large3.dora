//= vm-args "--gc=swiper --gc-verify"

class Foo(let x: Int32)

fun main() {
    let x = Array[Foo](16 * 1024);
    forceCollect();
    assert(x(0) === nil);

    x(0) = Foo(1I);
    forceMinorCollect();
    assert(x(0).x == 1I);

    x(16 * 1024 - 1) = Foo(2I);
    forceMinorCollect();
    assert(x(0).x == 1I);
    assert(x(16 * 1024 - 1).x == 2I);
}