//= vm-args "--gc=swiper --gc-verify"

class Foo

fn main() {
    let filler = Foo();
    let x = Array[Foo]::fill(16L * 1024L, filler);
    let y = Array[Foo]::fill(16L * 1024L, filler);
    let z = Array[Foo]::fill(16L * 1024L, filler);
    std::forceCollect();
    assert(x(0L) === filler);
    x(0L) = Foo();
    std::forceCollect();
    assert(x(0L) !== filler);
}
