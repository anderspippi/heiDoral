//= vm-args "--gc-parallel-minor --gc-worker=2 --gc-verify"

fun main() {
    let x = Foo(Option[Foo]::Some(Foo(Option[Foo]::None)));
    forceMinorCollect();
    assert(x.next.unwrap().next.isNone());
    forceMinorCollect();
    assert(x.next.unwrap().next.isNone());
    forceMinorCollect();
    assert(x.next.unwrap().next.isNone());
}

class Foo(let next: Option[Foo])
