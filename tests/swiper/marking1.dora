//= ignore

fn main() {
    let arr = Vec[Foo]();

    var i = 0i32;
    while i < 10i32 {
        arr.push(tree(9));
        i = i + 1i32;
    }

    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
}

fn tree(depth: Int32): Foo {
    if depth == 0i32 {
        return Foo(nil, nil, nil, nil, nil);
    }

    return Foo(
        tree(depth-1),
        tree(depth-1),
        tree(depth-1),
        tree(depth-1),
        tree(depth-1)
    );
}

class_new Foo(a: Foo, b: Foo, c: Foo, d: Foo, e: Foo)
