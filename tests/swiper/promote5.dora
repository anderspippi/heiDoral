//= vm-args "--gc=swiper --gc-verify"

class Foo(let x: Int32)

fun main() {
    let default = Foo(17);
    let x = Array::fill[Foo](64L, default);
    forceMinorCollect();
    assert(x(0L) === default);
    assert(x(63L) === default);

    x(0L) = Foo(1);
    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L) === default);

    x(63L) = Foo(2);
    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L).x == 2);

    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L).x == 2);
}
