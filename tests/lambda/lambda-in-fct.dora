//= vm-args "--gc=swiper --gc-verify"

fn main() {
    let l0: (): Int64 = ||: Int64 { 0 };
    let l1: (): Int64 = ||: Int64 { 1 };
    assert(myid(l0) === l0);
    assert(call_my_lambda(l0) == 0);
    assert(myid(l1) === l1);
    assert(call_my_lambda(l1) == 1);
    std::forceCollect();
}

fn myid(lambda: (): Int64): (): Int64 {
    lambda
}

fn call_my_lambda(lambda: (): Int64): Int64 {
    lambda()
}
