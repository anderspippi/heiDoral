fn main() {
  let b = B();
  assert(b.printBase() == 2I);
  assert(b.printSpecial() == 1I);

  let c = C();
  assert(c.printBase() == 2I);
  assert(c.printSpecial() == 1I);
}

@open @abstract class A {
  @abstract fn printSpecial(): Int32;
  fn printBase(): Int32 = 2I;
}

@open class B: A {
  @override fn printSpecial(): Int32 = 1I;
}

class C: B {}
