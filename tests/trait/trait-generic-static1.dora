fn main() {
    assert(f[Bar]() == 1i32);
    assert(f[Baz]() == 2i32);
}

trait X {
    @static fn foo(): Int32;
}

fn f[T: X](): Int32 {
    return T::foo();
}

class_new Bar

impl X for Bar {
    @static fn foo(): Int32 = 1i32;
}

class_new Baz

impl X for Baz {
    @static fn foo(): Int32 = 2i32;
}
