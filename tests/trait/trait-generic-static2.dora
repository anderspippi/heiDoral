fn main() {
    assert("x${f[Bar]()}y${f[Baz]()}z" == "xBAR1yBAZ2z");
}

trait X {
    @static fn foo(): Self;
}

fn f[T: X + std::Stringable](): String = T::foo().toString();

class Bar(let x: Int32)

impl std::Stringable for Bar {
    fn toString(): String = "BAR${self.x}";
}

impl X for Bar {
    @static fn foo(): Bar = Bar(1I);
}

class Baz(let x: Int32)

impl std::Stringable for Baz {
    fn toString(): String = "BAZ${self.x}";
}

impl X for Baz {
    @static fn foo(): Baz = Baz(2I);
}
