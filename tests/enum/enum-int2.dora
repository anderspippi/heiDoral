enum Foo { A, B }

impl Foo {
    fn toString(): String {
        if self == Foo::A {
            "A"
        } else {
            "B"
        }
    }

    fn toInt32(): Int32 {
        if self == Foo::A {
            0I
        } else {
            1I
        }
    }
}

fn main() {
    assert("A" == asString(Foo::A));
    assert("B" == asString(Foo::B));

    assert(0I == asInt(Foo::A));
    assert(1I == asInt(Foo::B));
}

fn asString(foo: Foo): String {
    foo.toString()
}

fn asInt(foo: Foo): Int32 {
    foo.toInt32()
}
