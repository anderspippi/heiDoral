fun main() {
    assert(NewOption[String]::None.isNone());
    assert(NewOption[Int32]::None.isNone());
    assert(!NewOption[String]::None.isSome());
    assert(!NewOption[Int32]::None.isSome());

    assert(!NewOption[String]::Some("abc").isNone());
    assert(!NewOption[Int32]::Some(12).isNone());
    assert(NewOption[String]::Some("abc").isSome());
    assert(NewOption[Int32]::Some(12).isSome());

    NewOption[String]::None.isNone();
    NewOption[Int32]::Some(12).isNone();
}
