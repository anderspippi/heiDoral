//= vm-args "--gc=copy"

enum Foo { A(Int32, String, Float32), B }

fun main() {
    let foo = createFoo();
    assert(std::unsafeLoadEnumVariant[Foo](foo) == 0);
    std::forceCollect();
    assert(std::unsafeLoadEnumVariant[Foo](foo) == 0);
}

fun createFoo(): Foo {
    Foo::A(112, "abc", 17.5F)
}
