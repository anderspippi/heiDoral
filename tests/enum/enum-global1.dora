//= vm-args "--gc=copy"

enum Foo { A(Int32, String, Float32), B }

var foo_value: Foo = Foo::B;

fun main() {
    assert(std::unsafeLoadEnumVariant[Foo](foo_value) == 1);
    foo_value = Foo::A(112, "abc", 17.5F);
    std::forceCollect();
    assert(std::unsafeLoadEnumVariant[Foo](foo_value) == 0);
    assert(std::unsafeLoadEnumElement[Foo, Int32](foo_value, 0, 0) == 112);
    assert(std::unsafeLoadEnumElement[Foo, String](foo_value, 0, 1) == "abc");
    assert(std::unsafeLoadEnumElement[Foo, Float32](foo_value, 0, 2) == 17.5F);
}
