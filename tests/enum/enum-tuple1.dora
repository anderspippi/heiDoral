//= vm-args "--gc=copy"

enum Foo { A(Int32, String, Float32), B }

fn main() {
    let tuple = createTuple();
    match tuple.0 {
        Foo::A(_, _, _) => {},
        Foo::B => unreachable[()](),
    }
    std::forceCollect();
    match tuple.0 {
        Foo::A(_, _, _) => {},
        Foo::B => unreachable[()](),
    }
}

fn createTuple(): (Foo, Float32) {
    (Foo::A(112i32, "abc", 17.5F), 2.5F)
}
