enum Foo[T] { A(T), B }

fun main() {
    let value = Foo[String]::A("abc");
    foo(value);
    assert(std::unsafeLoadEnumVariant[Foo[String]](value) == 0);
    assert(std::unsafeLoadEnumElement[Foo[String], String](value, 0, 0) == "abc");

    let value = Foo[String]::B;
    assert(std::unsafeLoadEnumVariant[Foo[String]](value) == 1);
}

fun foo(x: Foo[String]): Foo[String] {
    let y = x;
    y
}
