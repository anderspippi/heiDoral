//= vm-args "--gc-young-size=64M --max-heap-size=128M"
//= config region

class MyThread(): std::Thread {
    @override fn run() {
        allocator();
        println("done");
    }
}

fn main() {
    var i = 0i32;

    while i < 4i32 {
        let thread = MyThread();
        thread.start();
        i = i + 1i32;
    }

    allocator();
    println("done");
}

fn allocator() {
    var i = 0i32;

    while i < 10_000i32 {
        Foo();
        i = i + 1i32;
    }
}

class Foo