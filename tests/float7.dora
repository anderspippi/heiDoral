fn main() {
    let array = Array[Float32]::empty();
    assert(0i64 == array.size());

    let array = Array[Float32]::fill(10i64, 2F);
    assert(10i64 == array.size());
    assert(2F == array(0i64));
    assert(2F == array(9i64));

    let array = Array[Float64]::empty();
    assert(0i64 == array.size());

    let array = Array[Float64]::fill(12i64, 3D);
    assert(12i64 == array.size());
    assert(3D == array(0i64));
    assert(3D == array(11i64));
}