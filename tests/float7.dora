//= cannon

fun main() {
    let array = arrayEmpty[Float]();
    assert(0L == array.length());

    let array = arrayFill[Float](10L, 2F);
    assert(10L == array.length());
    assert(2F == array(0));
    assert(2F == array(9));

    let array = arrayEmpty[Double]();
    assert(0L == array.length());

    let array = arrayFill[Double](12L, 3D);
    assert(12L == array.length());
    assert(3D == array(0));
    assert(3D == array(11));
}