//= cannon

fun main() {
  let c = C();

  assert(c.foo(1I) == 3I);
  assert(c.foo(2I) == 5I);
}

@open class A {
  @open fun foo(a: Int32) -> Int32 {
    return a;
  }
}

@open class B: A {
  @open @override fun foo(a: Int32) -> Int32 {
    return super.foo(a) * 2I;
  }
}

class C: B {
  @override fun foo(a: Int32) -> Int32 {
    return super.foo(a) + 1I;
  }
}
