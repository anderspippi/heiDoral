fn main() {
  let c = C();

  assert(c.foo(1i32) == 3i32);
  assert(c.foo(2i32) == 5i32);
}

@open class A {
  @open fn foo(a: Int32): Int32 {
    return a;
  }
}

@open class B: A {
  @open @override fn foo(a: Int32): Int32 {
    return super.foo(a) * 2i32;
  }
}

class C: B {
  @override fn foo(a: Int32): Int32 {
    return super.foo(a) + 1i32;
  }
}
