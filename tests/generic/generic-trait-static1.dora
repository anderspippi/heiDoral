fn main() {
    assert(17I == bar[SomeClass]());
    assert(42I == bar[OtherClass]());
}

fn bar[T: Foo](): Int32 {
    T::baz()
}

trait Foo {
    @static fn baz(): Int32;
}

class SomeClass

impl Foo for SomeClass {
    @static fn baz(): Int32 = 17I;
}

class OtherClass

impl Foo for OtherClass {
    @static fn baz(): Int32 = 42I;
}
