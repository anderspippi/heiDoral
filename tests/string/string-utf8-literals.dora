fn main() {
    let val = "abc";
    assert(val.size() == 3i64);
    assert(val.codePoints().size() == 3i64);
    let it = val.codePoints();

    assert(it.next() == 'a');
    assert(it.toString() == "bc");
    assert(it.next() == 'b');
    assert(it.toString() == "c");
    assert(it.next() == 'c');
    assert(it.toString() == "");
    assert(it.hasNext() == false);

    let val = "üß";
    assert(val.size() == 4i64);
    assert(val.codePoints().size() == 2i64);
    let it = val.codePoints();

    assert(it.next().toInt32() == 0xFCI);
    assert(it.next().toInt32() == 0xDFI);
    assert(it.hasNext() == false);

    let it = val.codePoints();

    assert(it.next() == 'ü');
    assert(it.next() == 'ß');
    assert(it.hasNext() == false);
}
