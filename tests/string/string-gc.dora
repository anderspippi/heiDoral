//= stdout "abxy\n"

fun main() {
    let x = Array::empty[String]();
    assert(x.size() == 0L);

    let emptyString = "".clone();
    let x = Array::fill[String](10L, emptyString);
    assert(x.size() == 10L);

    x(0L) = "a" + "b";
    x(1L) = "x" + "y";

    forceCollect();

    print(x(0L));
    println(x(1L));

    assert(x(0L) == "ab");
    assert(x(1L) == "xy");
    assert(x(2L) === emptyString);
    assert(x(9L) === emptyString);
}