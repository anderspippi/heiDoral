//= vm-args "--gc-verify --gc=copy"

fun main() {
    let x = build_vec();
    forceCollect();
    assert(x.size() == 3L);
    pop_vec(x, "ghi");
    forceCollect();
    pop_vec(x, "def");
    forceCollect();
    pop_vec(x, "abc");
    forceCollect();
    assert(x.size() == 0L);
}

fun build_vec(): Vec[(Int32, String)] {
    Vec[(Int32, String)]((1, "a" + "b" + "c"), (2, "d" + "e" + "f"), (3, "g" + "h" + "i"))
}

fun pop_vec(vec: Vec[(Int32, String)], expected: String) {
    let (_, str) = vec.pop();
    assert(str == expected);
}