fn main() {
  insertFirst();
  insertFirstNoResize();
  insertMiddle();
  insertMiddleNoResize();
  insertLast();
  insertLastNoResize();
  insertEmpty();
}

fn insertFirst() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.insertAt(0L, -23I);
  assert(xs == Vec[Int32](-23I, 1I, 2I, 3I));
}

fn insertFirstNoResize() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.removeAt(0L);
  xs.insertAt(0L, -23I);
  assert(xs == Vec[Int32](-23I, 2I, 3I));
}

fn insertMiddle() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.insertAt(1L, -23I);
  assert(xs == Vec[Int32](1I, -23I, 2I, 3I));
}

fn insertMiddleNoResize() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.removeAt(1L);
  xs.insertAt(1L, -23I);
  assert(xs == Vec[Int32](1I, -23I, 3I));
}

fn insertLast() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.insertAt(xs.size(), -23I);
  assert(xs == Vec[Int32](1I, 2I, 3I, -23I));
}

fn insertLastNoResize() {
  let xs = Vec[Int32](1I, 2I, 3I);
  xs.removeAt(xs.size() - 1L);
  xs.insertAt(xs.size(), -23I);
  assert(xs == Vec[Int32](1I, 2I, -23I));
}

fn insertEmpty() {
  let xs = Vec[Int32]();
  xs.insertAt(0L, -23I);
  assert(xs == Vec[Int32](-23I));
}
