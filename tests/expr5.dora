fn main() {
  Bar(1I).exec();
}

class Bar(var a: Int32) {
  fn exec() {
    Foo(self.f()).a = self.g();
    assert(self.a == 100I);
  }

  fn f(): Int32 {
    assert(self.a == 1I);
    self.a = 10I;

    return self.a;
  }

  fn g(): Int32 {
    assert(self.a == 10I);
    self.a = 100I;

    return self.a;
  }
}

class Foo(var a: Int32)
