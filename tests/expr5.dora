fn main() {
  Bar(1i32).exec();
}

class Bar(var a: Int32) {
  fn exec() {
    Foo(self.f()).a = self.g();
    assert(self.a == 100i32);
  }

  fn f(): Int32 {
    assert(self.a == 1i32);
    self.a = 10i32;

    return self.a;
  }

  fn g(): Int32 {
    assert(self.a == 10i32);
    self.a = 100i32;

    return self.a;
  }
}

class Foo(var a: Int32)
