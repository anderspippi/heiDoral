//= stdout file

fun main() {
    var i = 1;

    while i <= 100 {
        if i % 15 == 0 {
            println("FizzBuzz");

        } else if i % 5 == 0 {
            println("Buzz");

        } else if i % 3 == 0 {
            println("Fizz");
        } else {
            println(i.toString());
        }

        i = i + 1;
    }
}