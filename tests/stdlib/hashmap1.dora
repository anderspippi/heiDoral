fun main() {
    let map = HashMap[Int, String]();
    map.insert(1, "a");
    map.insert(10000, "b");
    map.insert(7, "c");

    {
        let it = map.makeIterator();
        while it.hasNext() {
            let key = it.nextKey();
            let value = it.nextValue();

            if key == 1 {
                assert(value == "a");
            } else if key == 10000 {
                assert(value == "b");
            } else if key == 7 {
                assert(value == "c");
            } else {
                unreachable();
            }
        }
    }

    assert(map.get(1).unwrap() == "a");
    assert(map.get(10000).unwrap() == "b");
    assert(map.get(7).unwrap() == "c");
    assert(map.get(2).isNone());
    assert(map.length() == 3);
    assert(map.isEmpty().not());

    assert(map.contains(1));
    assert(map.contains(10000));
    assert(map.contains(7));
    assert(!map.contains(2));

    map.remove(10000);

    assert(map.get(1).unwrap() == "a");
    assert(map.get(10000).isNone());
    assert(map.get(7).unwrap() == "c");
    assert(map.get(2).isNone());
    assert(map.length() == 2);
    assert(map.isEmpty().not());

    assert(map.contains(1));
    assert(!map.contains(10000));
    assert(map.contains(7));

    {
        let it = map.makeIterator();
        while it.hasNext() {
            let key = it.nextKey();
            let value = it.nextValue();

            if key == 1 {
                assert(value == "a");
            } else if key == 7 {
                assert(value == "c");
            } else {
                unreachable();
            }
        }
    }
}
