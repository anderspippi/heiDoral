//= cannon

fun main() {
    let set = HashSet[Int32]();
    set.insert(1I);
    set.insert(10000I);
    set.insert(7I);

    assert(set.length() == 3L);
    assert(set.isEmpty().not());

    assert(set.contains(1I));
    assert(set.contains(10000I));
    assert(set.contains(7I));
    assert(!set.contains(2I));

    set.remove(10000I);

    assert(set.length() == 2L);
    assert(set.isEmpty().not());

    assert(set.contains(1I));
    assert(!set.contains(10000I));
    assert(set.contains(7I));
}
