//= cannon

fun main() {
    let x = Vec[Int32]();

    x.push(1);
    x.push(2);
    x.push(3);

    assert(3 == x.removeAt(2L));
    assert(x(0L) == 1);
    assert(x(1L) == 2);
    assert(x.length() == 2L);

    assert(1 == x.removeAt(0L));
    assert(x(0L) == 2);
    assert(x.length() == 1L);

    assert(2 == x.removeAt(0L));
    assert(x.length() == 0L);
}
