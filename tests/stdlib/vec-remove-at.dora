//= cannon

fun main() {
    let x = Vec[Int32]();

    x.push(1I);
    x.push(2I);
    x.push(3I);

    assert(3I == x.removeAt(2L));
    assert(x(0L) == 1I);
    assert(x(1L) == 2I);
    assert(x.length() == 2L);

    assert(1I == x.removeAt(0L));
    assert(x(0L) == 2I);
    assert(x.length() == 1L);

    assert(2I == x.removeAt(0L));
    assert(x.length() == 0L);
}
