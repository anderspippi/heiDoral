fun main() {
    let map = hashMap[Int32, String]((1I, "a"), (2I, "b"), (3I, "c"), (4I, "d"));
    assert(map.length() == 4L);

    assert(map.contains(1I));
    assert(map.contains(2I));
    assert(map.contains(3I));
    assert(map.contains(4I));
    assert(!map.contains(0I));

    assert(map.get(1I).unwrap() == "a");
    assert(map.get(2I).unwrap() == "b");
    assert(map.get(3I).unwrap() == "c");
    assert(map.get(4I).unwrap() == "d");
}
