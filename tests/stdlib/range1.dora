fun main() {
    let x = range(0, 5).makeIterator();

    assert(x.hasNext());
    assert(x.next() == 0);

    assert(x.hasNext());
    assert(x.next() == 1);

    assert(x.hasNext());
    assert(x.next() == 2);

    assert(x.hasNext());
    assert(x.next() == 3);

    assert(x.hasNext());
    assert(x.next() == 4);

    assert(!x.hasNext());
}
