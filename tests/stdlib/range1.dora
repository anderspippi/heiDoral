fn main() {
    let x = std::range(0i32, 5i32).makeIterator();

    assert(x.hasNext());
    assert(x.next() == 0i32);

    assert(x.hasNext());
    assert(x.next() == 1i32);

    assert(x.hasNext());
    assert(x.next() == 2i32);

    assert(x.hasNext());
    assert(x.next() == 3i32);

    assert(x.hasNext());
    assert(x.next() == 4i32);

    assert(!x.hasNext());
}
