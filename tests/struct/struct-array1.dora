struct Foo {
    f1: Int32,
    f2: Int32,
}

fun main() {
    let arr = Array::fill[Foo](10L, Foo(10, 17));
    assert(arr.size() == 10L);
    assert(arr(0L).f1 == 10);
    assert(arr(0L).f2 == 17);

    arr(0L) = Foo(100, 170);
    assert(arr(0L).f1 == 100);
    assert(arr(0L).f2 == 170);

    let arr = Array::empty[Foo]();
    assert(arr.size() == 0L);
}
