//= vm-args "--gc=copy --disable-tlab --gc-stress"

fn main() {
    let x = tpl1(42I);
    std::forceCollect();
    assert(x.second.value == 17I);
}

struct Pair(first: Int32, second: Foo)

fn tpl1(value: Int32): Pair {
    assert(value == 42I);
    Pair(1I, Foo(17I))
}

class Foo(let value: Int32)
