//= vm-args "--gc=copy --disable-tlab --gc-stress"

fn main() {
    let x = id[Bar](Bar("f" + "o" + "o", Foo(17I)));
    std::forceCollect();
    assert(x.first == "foo");
    assert(x.second.value == 17I);
}

struct Bar(first: String, second: Foo)
class Foo(let value: Int32)

fn id[T](x: T): T { x }
