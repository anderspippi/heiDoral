//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo(let value: Int32) {
    std::forceCollect();
}

struct Bar(f1: Foo, f2: Foo, f3: Foo)

fn main() {
    assert(tpl1() == 1I);
    assert(tpl2() == 2I);
    assert(tpl3() == 3I);
    assert(tplsum() == 6I);
}

fn tpl1(): Int32 {
    let x = Bar(Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.f1.value
}

fn tpl2(): Int32 {
    let x = Bar(Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.f2.value
}

fn tpl3(): Int32 {
    let x = Bar(Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.f3.value
}

fn tplsum(): Int32 {
    let x = Bar(Foo(1I), Foo(2I), Foo(3I));
    std::forceCollect();
    x.f1.value + x.f2.value + x.f3.value
}
