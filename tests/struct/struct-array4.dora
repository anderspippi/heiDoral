//= vm-args "--gc=copy --disable-tlab --gc-stress"

struct Foo {
    f1: Int32,
    f2: Bar,
}

class Bar(let value: Int32)

fun main() {
    let x = Array::fill[Foo](10L, Foo(0, Bar(0)));
    x(0L) = Foo(1, Bar(1));
    x(4L) = Foo(2, Bar(2));
    std::forceCollect();
    assert(x(0L).f1 == x(0L).f2.value);
    assert(x(4L).f1 == x(4L).f2.value);
}
