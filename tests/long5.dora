//= cannon

fun main() {
    let ba = Array[Int64](0);
    assert(ba.length() == 0);

    let ba = arrayFill[Int64](10L, 2L);
    assert(ba.length() == 10);

    ba(0) = 10L;
    ba(1) = 11L;
    ba(2) = 12L;

    assert(10L == ba(0));
    assert(11L == ba(1));
    assert(12L == ba(2));
    assert(2L == ba(9));
}
