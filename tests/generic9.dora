fn main() {
    let a = A[Int32](0i32);
    consume(a.x);

    let b = A[String]("");
    consume2(b.x);
}

class A[T](default: T) {
    var x: Array[T] = Array[T]::fill(10i64, default);
}

fn consume(x: Array[Int32]) {
    assert(x(9i64) == 0i32);
}

fn consume2(x: Array[String]) {
    assert(x.size() == 10i64);
}
