@pub class Locale(let value: String) {
  @pub @static fn de(): Locale = Locale("de");
  @pub @static fn en(): Locale = Locale("en");
  @pub @static fn fr(): Locale = Locale("fr");
}

impl Equals for Locale {
  fn equals(rhs: Locale): Bool = self.value == rhs.value;
}

impl Stringable for Locale {
  fn toString(): String = "Locale(" + self.value + ")";
}
